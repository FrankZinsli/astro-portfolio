---
import MetaHead from '../components/layout/MetaHead.astro';
import Header from '../components/layout/Header.astro';
import Footer from '../components/layout/Footer.astro';
import Counter from '../components/vue/counter.vue';

export interface Props {
	title: string;
	description: string
}
const { title } = Astro.props;
const { description } = Astro.props;
---

<!DOCTYPE html>
<html lang="de_CH">
	<MetaHead title={title} description={description}></MetaHead>
	<body>
		<Header></Header>
		<slot />
		<Counter client:visible>
			<h1>Hello</h1>
		</Counter>
		<Footer></Footer>
	</body>
</html>

<style is:global>

</style>
